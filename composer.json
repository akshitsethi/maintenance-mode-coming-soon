{
  "name": "akshitsethi/maintenance-mode-coming-soon",
  "description": "Maintenance mode & coming soon plugin for WordPress that actually works.",
  "keywords": [
    "wordpress",
    "plugin",
    "coming soon",
    "maintenance mode"
  ],
  "type": "wordpress-plugin",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2"
  },
  "autoload": {
    "psr-4": {
      "AkshitSethi\\Plugins\\MaintenanceMode\\": "includes"
    }
  },
    "require-dev": {
      "10up/wp_mock": "^0.4.2",
      "phpunit/phpunit": "^9.5",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
    "squizlabs/php_codesniffer": "^3.6",
    "wp-coding-standards/wpcs": "^2.3",
    "antecedent/patchwork": "^2.1",
    "phpro/grumphp": "^1.11"
  },
  "scripts": {
    "lint": "phpcs .",
    "lint-fix": "phpcbf . "
  }
}
